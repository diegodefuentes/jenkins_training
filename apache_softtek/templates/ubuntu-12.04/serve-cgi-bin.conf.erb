
		ScriptAlias /cgi-bin/ <%= node['apache']['ScriptsDirectory'] %>
		<Directory "<%= node['apache']['ScriptsDirectory'] %>">
			AllowOverride <%= node['apache']['OptionAllowOverride'] %>
			Options +ExecCGI -MultiViews +SymLinksIfOwnerMatch
			Require all granted
		</Directory>

